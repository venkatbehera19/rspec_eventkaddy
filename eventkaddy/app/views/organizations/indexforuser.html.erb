<h3>Listing Organizations</h3>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Url web</th>
    <th>Url twitter</th>
    <th>Url facebook</th>
    <!--th></th>
    <th></th>
    <th></th-->
  </tr>

<% @organizations.each do |organization| %>
  <tr>
    <td><%= link_to organization.name, "/events/indexfororg/#{organization.id}" %></td>
    <td><%= organization.description %></td>
    <td><%= organization.url_web %></td>
    <td><%= organization.url_twitter %></td>
    <td><%= organization.url_facebook %></td>
    <!--td><%= link_to 'Show', organization ,class:"btn show" %></td>
    <td><%= link_to 'Edit', edit_organization_path(organization) ,class:"btn edit" %></td>
    <td><%= link_to 'Delete', organization, :confirm => 'Are you sure?', :method => :delete ,class:"btn delete" %></td-->
  </tr>
<% end %>
</table>

<br />