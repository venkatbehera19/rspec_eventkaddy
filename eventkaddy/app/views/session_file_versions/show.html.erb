<h4>Session File: Versions</h4>

<%= link_to 'New File Version', "/session_file_versions/#{@session_file_id}/new" ,class:"btn btn-primary" %> <br/><br/>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  <tr>
    <th>File Name</th>
    <th>Submitted By</th>
    <th>Final Version</th>
    <th>Updated At</th>
    <th></th>
    <th></th>
  </tr>

<% @session_file_versions.each do |session_file_version| %>
  <tr>
    <td><%= session_file_version.event_file.name %></td>
    <td><%= session_file_version.user.email %></td>
    <td><%= session_file_version.final_version %></td>
    <td><%= session_file_version.updated_at %></td>
    <td><%= link_to 'View/Download', session_file_version.event_file.path, class:"btn show" %></td>
    <td><%= link_to 'Delete', session_file_version, :confirm => 'Are you sure?', :method => :delete ,class:"btn delete" %></td>
  </tr>
<% end %>
</table>


