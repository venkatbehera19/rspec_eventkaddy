<%= link_to 'Back', location_mappings_path, :class=> "btn btn-link" %>

<h3>Select Map to Add Coordinates To</h3>

<table id="location_mappings" cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
<!--
	<col width="200" />
	<col width="100" />
	<col width="35" />
	<col width="35" />
	<col width="35" />
-->

<thead>
  <tr>
    <th>Map Name</th>
    <th>Type</th>
    <th></th>
  </tr>
</thead>

<tbody>
<% @maps.each do |map| %>
  <tr>
    <td><%= map.name %></td>
     <td><%= map.map_type.map_type %></td>
    <td><%= link_to 'Show', "/location_mappings/set_coordinates/#{map.id}" ,class:"btn btn-outline-info" %></td>
  </tr>
<% end %>
</tbody>

</table>

<br />
