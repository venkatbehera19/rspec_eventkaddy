<h1>Listing map_types</h1>
<%= link_to 'New Map type', new_map_type_path,class:"btn btn-primary" %> <br/><br/>

<table class="table">
  <tr>
    <th>Map type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @map_types.each do |map_type| %>
  <tr>
    <td><%= map_type.map_type %></td>
    <td><%= link_to 'Show', map_type ,class:"btn btn-outline-info" %></td>
    <td><%= link_to 'Edit', edit_map_type_path(map_type) ,class:"btn btn-outline-success" %></td>
    <td><%= link_to 'Delete', map_type, data: {:confirm => 'Are you sure?'}, :method => :delete ,class:"btn btn-outline-danger" %></td>
  </tr>
<% end %>
</table>

<br />

