<h1>Listing home_button_entries</h1>
<%= link_to 'New Home button entry', new_home_button_entry_path,class:"btn btn-primary" %> <br/><br/>

<table id="home_button_entries" cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  <tr>
    <th>Group</th>
    <!--<th>Render url</th>-->
    <th>Name</th>
    <!--<th>Icon entry</th>-->
    <th>Content</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @home_button_entries.each do |home_button_entry| %>
  <tr>
    <td><%= home_button_entry.group_id %></td>
    <!--<td><%= home_button_entry.render_url %></td>-->
    <td><%= home_button_entry.name %></td>
    <!--<td><%= home_button_entry.icon_entry %></td>-->
    <% if home_button_entry.content!=nil then %>
    <td><%= home_button_entry.content.truncate(100) %></td>
    <% end %>
    <td><%= home_button_entry.position %></td>
    <td><%= link_to 'Show', home_button_entry ,class:"btn show" %></td>
    <td><%= link_to 'Edit', edit_home_button_entry_path(home_button_entry) ,class:"btn edit" %></td>
    <td><%= link_to 'Delete', home_button_entry, :confirm => 'Are you sure?', :method => :delete ,class:"btn delete" %></td>
  </tr>
<% end %>
</table>

<br />


