<div class="dropdown">
  <div
    class="btn report-dropdown dropdown-toggle"
    data-toggle="dropdown"
  >
    CE Sessions Downloads
  </div>
  <div class="dropdown-menu report-dropdown-list">
    <%= link_to "Export My CE Sessions To Spreadsheet", "/attendees/#{@attendee.id}/ce_sessions_report", class: "dropdown-item" %>
  </div>
</div>
<br>
 <h4 class="view-title">My CE Sessions</h4>
<br />
<br />
<table
    id="cms-my-ce-sessions-table"
    cellpadding="0"
    cellspacing="0"
    border="0"
    class="table table-striped table-bordered"
  >
    <thead>
      <th>session code</th>
      <th>session title</th>
      <th>date</th>
      <th>start time</th>
      <th>end time</th>
    </thead>
  <% !@my_ce_sessions.blank? && @my_ce_sessions.each do |session| %>
    <tr>
      <td><%= session[:session_code] %></td>
      <td><%= session[:title] %></td>
      <td><%= session[:date] %></td>
      <td><%= get_local_time(session[:start_at]) %></td>
      <td><%= get_local_time(session[:end_at]) %></td>
    </tr>
    <% end %>
  </table>
<script>
  $(function () {
    $.extend($.fn.dataTableExt.oStdClasses, {
      sWrapper: 'dataTables_wrapper form-inline table-responsive'
    })
    $('#cms-my-ce-sessions-table').dataTable({
      sDom: "<'pull-left'l><'ml-auto mr-1'f>rt<'pull-left'i><'ml-auto'p>",
    })
    $('dataTables_filter').css('display', 'none')
    return $('table.dataTable th').css('max-width', '97% !important')
  })
</script>