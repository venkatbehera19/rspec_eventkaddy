<h3>Listing Events for Organization:<b> <%= @organization_for_events.name %></b></h3>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Event start at</th>
    <th>Event end at</th>
    <th>Exhibitors</th>
    <th>Enhanced listings</th>
    <th>Sponsorship</th>
    <!--th></th>
    <th></th>
    <th></th-->
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= link_to event.name, "/events/#{@organization_for_events.id}/#{event.id}/configure" %></td>
    <td><%= event.description %></td>
    <td><%= event.event_start_at %></td>
    <td><%= event.event_end_at %></td>
    <td><%= event.exhibitors %></td>
    <td><%= event.enhanced_listings %></td>
    <td><%= event.sponsorship %></td>
    <!--td><%= link_to 'Show', event ,class:"btn show" %></td-->
    <td><%= link_to 'Edit', edit_event_path(event) ,class:"btn edit" %></td>
    <!--td><%= link_to 'Delete', event, :confirm => 'Are you sure?', :method => :delete ,class:"btn delete" %></td-->
  </tr>
<% end %>
</table>

<br />